{
  "name": "phapi/serializer-json",
  "description": "Phapi JSON Serializer is a middleware based serializer that converts arrays to JSON. The middleware are PSR-7 compliant and a package developed for and used by the Phapi Framework.",
  "keywords": [
    "middleware", "serializer", "json", "phapi"
  ],
  "version": "1.0.0",
  "minimum-stability": "stable",
  "license": "MIT",
  "homepage": "http://phapi.github.io",
  "authors": [
    {
      "name": "Peter Ahinko",
      "email": "peter@ahinko.se",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "phapi/contract": "dev-master",
    "phapi/http": "dev-master",
    "phapi/serializer": "dev-master",
    "phapi/exception": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "mockery/mockery": "0.9.*",
    "codeclimate/php-test-reporter": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Phapi\\Middleware\\Deserializer\\Json\\": "src/Phapi/Middleware/Deserializer/Json/",
      "Phapi\\Middleware\\Serializer\\Json\\": "src/Phapi/Middleware/Serializer/Json/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Phapi\\Tests\\Middleware\\Deserializer\\Json\\": "tests/Phapi/Middleware/Deserializer/Json/",
      "Phapi\\Tests\\Middleware\\Serializer\\Json\\": "tests/Phapi/Middleware/Serializer/Json/"
    }
  }
}